{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "test.vue",
      "type": "node",
      "request": "launch",
      "runtimeArgs": ["--nolazy","--loader", "ts-node/esm"],
      "args": ["src/index.ts", "./tests/test.vue"],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
    },
    {
      "name": "Test commands",
      "type": "node",
      "request": "launch",
      "runtimeArgs": ["--nolazy","--loader", "ts-node/esm"],
      "args": ["src/index.ts", "-n", "--nuxt", "-o", "lol", "./tests/test.vue"],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
    },
    {
      "name": "Upgrade current Vue file",
      "type": "node",
      "request": "launch",
      "args": ["index.ts", "${relativeFile}"],
      "runtimeArgs": ["--nolazy", "--loader", "ts-node"],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
    },
  ]
}